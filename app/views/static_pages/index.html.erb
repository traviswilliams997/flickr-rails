<h1>StaticPages#index</h1>
<p>Find me in app/views/static_pages/index.html.erb</p>

<%= form_with url: "/", method: :get do |form| %>
  <%= form.label :flickr_id, "Flicker User ID:" %>
  <%= form.text_field :flickr_id %>
  <%= form.submit "Get Photos" %>
<% end %>

<%  urls = Url.get_urls %>
    <% urls.each do |url| %>

      <%= image_tag "#{url}" %>
    
    <% end %>



<%= urls %> <br>
<%= @response %>



  